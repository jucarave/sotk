          <argument>
            <kind>1</kind>
            <string>// Fade In - Out (transfer between rooms)
if (fade == 1){
    transferAlpha += 0.1;
    if (transferAlpha &gt;= 1.0){
        transferAlpha = 1.0;
        if (audio_is_playing(sndOpenDoor)){ exit; }
        room_goto(transferTo[0]);
        fade = 2;
    }
}else if (fade == 2){
    var player = instance_create(transferTo[1], transferTo[2], objPlayer);
    player.direction = transferTo[3];
    fade = 3;
    audio_play_sound(sndCloseDoor, 5, false);
}else if (fade == 3){
    transferAlpha -= 0.1;
    if (transferAlpha &lt;= 0){
        transferAlpha = 0;
        fade = 0;
    }
}
</string>
